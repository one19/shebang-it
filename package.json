{
  "name": "shebang-it",
  "version": "0.0.4",
  "description": "shebang-it",
  "keywords": [
    "cli",
    "util",
    "node",
    "shebang",
    "compile",
    "brightsole"
  ],
  "main": "index.js",
  "repository": "git@github.com:one19/shebang-it.git",
  "author": "Drew Showalter <maelstroswim@gmail.com>",
  "license": "MIT",
  "bin": {
    "shebang-it": "bin/index.js"
  },
  "files": [
    "bin/index.js",
    "dist/index.js",
    "dist/index.map"
  ],
  "scripts": {
