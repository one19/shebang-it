  "devDependencies": {
    "@brightsole/eslint-config-lint-node": "0.0.4",
    "ava": "^1.2.1",
    "husky": "^0.14.2",
    "lint-staged": "^7.2.2",
    "mock-fs": "^4.6.0",
    "nyc": "^13.3.0",
    "parcel-bundler": "^1.8.1",
    "shebang-it": "^0.0.3"
  },
  "dependencies": {
    "commander": "^2.11.0",
    "lodash": "^4.17.11",
    "signale": "^1.2.1"
